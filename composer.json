{
  "name": "artsoft/yii2-fullcalendar-scheduler",
  "description": "Yii2 widget for fullcalendar scheduler module",
  "keywords": [
    "fullcalendar-scheduler",
    "scheduler",
    "javascript",
    "event",
    "calendar"
  ],
  "homepage": "https://github.com/artsoft/yii2-fullcalendar-scheduler",
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Edo Freriks",
      "email": "edofre@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "yiisoft/yii2": "*",
    "yiisoft/yii2-jui": "*",
    "bower-asset/fullcalendar": "v3.9.0",
    "bower-asset/fullcalendar-scheduler": "v1.9.4"
  },
  "require-dev": {
    "codeception/codeception": "*"
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "repositories": {
        "bower-asset/eve": {
          "type": "bower-vcs",
          "url": "https://github.com/adobe-webplatform/eve.git"
        }
      }
    }
  },
  "autoload": {
    "psr-4": {
      "artsoft\\fullcalendarscheduler\\": "src"
    }
  }
}